<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>jp.radio-calisthenics-together.monitor</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>scripts/health_monitor.py</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>45</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>{{REPO_DIR}}/logs/monitor_stdout.log</string>
    <key>StandardErrorPath</key>
    <string>{{REPO_DIR}}/logs/monitor_stderr.log</string>
    <key>WorkingDirectory</key>
    <string>{{REPO_DIR}}</string>
</dict>
</plist>
